# ACC_Telemetry环境变量配置示例
# 复制此文件为.env并根据需要修改

# 应用程序配置
APP_TITLE=ACC Telemetry
APP_WIDTH=1200
APP_HEIGHT=800
APP_DEBUG=False
APP_LOG_LEVEL=INFO

# Web服务器配置
WEB_SERVER_HOST=0.0.0.0
WEB_SERVER_PORT=8080

# OSC配置
OSC_HOST=127.0.0.1
OSC_PORT=9000

# 音乐引擎配置
MUSIC_ENGINE_TYPE=supercollider
SUPERCOLLIDER_PATH=/path/to/supercollider

# 数据记录配置
ENABLE_DATA_LOGGING=False
DATA_LOG_PATH=./logs